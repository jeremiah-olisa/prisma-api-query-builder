{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "prisma-api-query-builder",
  "module": "dist/prisma-api-query-builder.esm.js",
  "repository": "https://github.com/jeremiah-olisa/prisma-api-query-builder.git",
  "author": "Jeremiah Olisa <jeremiaholisa453@gmail.com>",
  "size-limit": [
    {
      "path": "dist/api-query-string-builder-index.js",
      "limit": "12 KB"
    },
    {
      "path": "dist/index.js",
      "limit": "40 KB"
    },
    {
      "path": "dist/query-builder.constant.js",
      "limit": "2 KB"
    },
    {
      "path": "dist/query-builder.decorator.js",
      "limit": "2 KB"
    },
    {
      "path": "dist/query-string-parser.js",
      "limit": "2 KB"
    },
    {
      "path": "dist/query-builder.js",
      "limit": "40 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@size-limit/webpack": "^8.1.0",
    "@size-limit/webpack-why": "^8.1.0",
    "@types/lodash": "^4.14.191",
    "@types/qs": "^6.9.7",
    "husky": "^8.0.2",
    "rimraf": "^3.0.2",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "qs": "^6.11.0"
  }
}
